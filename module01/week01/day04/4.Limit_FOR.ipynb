   "source": [
    "import math\n",
    "\n",
    "def compute_y(x):\n",
    "    return math.sqrt(1 - x*x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "75ffc583-6b70-40e1-a242-c487de76960c",
   "metadata": {},
   "outputs": [],
   "source": [
    "delta_x = 0.01\n",
    "n = int(2 / delta_x)\n",
    "\n",
    "x = -1.0\n",
    "half_area = 0.0\n",
    "for _ in range(n):\n",
    "    y = compute_y(x)\n",
    "    half_area = half_area + delta_x*y\n",
    "    x = x + delta_x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "299b5260-89cb-4a34-8d4f-d15bcb6a1bb0",
